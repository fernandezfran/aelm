# HD input for GEMS

# init config
>+ read initial.xyz
#-STD->           76 particles in system
#-STD->           76 particles selected

# simulation box
a:=10.566048
box size $a $a $a
nb_dcut=0.5

# pbc
> all
#-STD->           76 particles selected
group 1 add
#-STD->           76 particles in group           1
set pbc t t t
box move 0.5 0.5 0.5

# call DFTB+
interact 1 dftbp dftb_in.hsd dftb.out
#-STD-> The interaction label is :i1
# DFTBP DFTB+ version:release 21.1
# DFTBP API version:0200
# NHB Not using linked cells since interaction does not have rcut.

# output files
outfile :f1 name thermo.$jobname$.dat
#-STD-> The outfile label is :f1
outfile :f1 cols energy 1
outfile :f1 each 10
outfile :f1 flush on

outfile :f2 name traj.$jobname$.xyz
#-STD-> The outfile label is :f2
outfile :f2 Poscr 1
outfile :f2 each 10
outfile :f2 flush on

